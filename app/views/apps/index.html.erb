<h1>Select an App to review!</h1>
<% if @apps.blank? %>
  <p>Ooops! No data to display!!</p>
<% else %>
  <% @apps.each do |current_app| %>
    <a href="/apps/<%= current_app.id %>"><%= current_app.app_name %></a><br>

    <br><p> created by 
    <img src="<%= current_app.app_image %>" height="100px">
    <p><%= link_to "Delete", "/apps/#{current_app.id}/delete", method: :delete %></p>
    <br><p>--------------------------------</p>
  <% end %>
<% end %>  
